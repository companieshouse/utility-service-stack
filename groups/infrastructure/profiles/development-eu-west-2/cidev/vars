environment = "cidev"
aws_profile = "development-eu-west-2"
enable_container_insights = true

ec2_key_pair_name = "chs-cidev"

# EC2 sizing and scaling options
asg_max_instance_count = 0 # allow cidev to scale up
asg_min_instance_count = 0 # allow cidev to scale down to 0
asg_desired_instance_count = 0 # keep cidev at 0 instance count until ec2 launch type service is needed